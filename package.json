{
  "name": "simple-chat",
  "displayName": "Simple Chat",
  "description": "Простой чат, который всегда отвечает '1'",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "simple-chat-sidebar",
          "title": "Simple Chat",
          "icon": "media/chat.svg"
        }
