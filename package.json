  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.18.34",
    "eslint": "^8.39.0",
    "typescript": "^5.0.4"
  }
}
