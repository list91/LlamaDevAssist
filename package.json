{
  "name": "chatbrancheditor",
  "displayName": "Chat Branch Editor",
  "description": "Расширение для редактирования сообщений в чате с поддержкой ветвления",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatbranch.start",
        "title": "Chat Branch: Начать чат"
      },
      {
        "command": "chatbranch.edit",
        "title": "Chat Branch: Редактировать сообщение"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.18.34",
    "eslint": "^8.39.0",
    "typescript": "^5.0.4"
  }
}
